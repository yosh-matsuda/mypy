from a import valid

def func() -> int:
    return 2

aliased_func = func
int_value = 1

class A:
    cls_var = valid

    def __init__(self, arg: str) -> None:
        self.self_var = arg

    def meth(self) -> None:
        func_value = int_value

    alias_meth = meth
    alias_func = func
    alias_alias_func = aliased_func
    int_value = int_value

[file a.py]
valid : list[int] = [1, 2, 3]


