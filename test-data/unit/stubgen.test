from _typeshed import Incomplete
from a import valid

def func() -> int: ...
aliased_func = func
int_value: int

class A:
    cls_var = valid
    self_var: Incomplete
    def __init__(self, arg: str) -> None: ...
    def meth(self) -> None: ...
    alias_meth = meth
    alias_func = func
    alias_alias_func = aliased_func
    int_value = int_value
